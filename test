class animal:
    patas = 8
    dedos = []

    @staticmethod
    def caminar():
        print("camina")

    @classmethod
    def saltar(cls):
        cls.caminar()


animal.saltar()
a = animal()
a.patas = 9
b = animal()


def sexo():
    pass


animal.sexo = sexo

